        <body>
            <p>This TEI customization describes the schema used for texts of the Austrian Baroque Corpus ABaC:us.</p>
            <schemaSpec ident="abacus" docLang="en" prefix="tei_" xml:lang="en" start="TEI teiCorpus">
                <moduleRef key="core"
                    except="abbr add analytic biblScope binaryObject cb citedRange del desc distinct divGen emph expan gb gloss graphic headItem headLabel hi index listBibl measureGrp media meeting mentioned num q relatedItem said soCalled sp speaker stage street textLang time unclear editor email label postCode postBox"/>
                <moduleRef key="corpus" except=""/>
                <moduleRef key="tei" except=""/>
                <moduleRef key="header" except="abstract appInfo application authority biblFull cRefPattern calendar calendarDesc catDesc category catRef revisionDesc revisionDesc listChange change classCode classDecl creation docEdition geoDecl handNote listPrefixDef refState prefixDef principal punctuation quotation refsDecl"/>
                <moduleRef key="textstructure"
                    except="argument closer dateline div1 div2 div3 div4 div5 div6 div7 floatingText group opener postscript salute signed trailer"/>
                <moduleRef key="gaiji" except="glyph glyphName localName unicodeName"/>
                <moduleRef key="transcr"
                    except="addSpan am damage damageSpan delSpan ex facsimile handNotes handShift line listTranspose metamark mod redo restore retrace sourceDoc space subst substJoin surface surfaceGrp surplus transpose undo zone"/>
                <moduleRef key="analysis" except="c cl m phr s span spanGrp"/>
                <moduleRef key="msdescription"
                    except="accMat acquisition additions adminInfo altIdentifier binding bindingDesc catchwords collation colophon condition custEvent custodialHist decoDesc decoNote depth dim dimensions explicit filiation finalRubric foliation handDesc height heraldry history incipit layout layoutDesc locus locusGrp material msContents msItem msItemStruct msName msPart musicNotation objectDesc objectType origDate origPlace origin physDesc provenance recordHist rubric scriptDesc seal sealDesc secFol signatures source stamp summary support supportDesc typeDesc watermark width"/>
                <moduleRef key="figures" except="notatedMusic figDesc formula"/>
                <moduleRef key="namesdates"
                    except="addName affiliation age birth bloc climate country death district education event faith floruit forename genName geo geogFeat geogName langKnowledge langKnown listEvent listNym listOrg listPerson listPlace listRelation location nameLink nationality nym occupation offset org orgName person personGrp place population region relation residence roleName sex socecStatus state surname terrain trait"/>
                <moduleRef key="linking"
                    except="ab alt altGrp anchor join joinGrp link linkGrp timeline when"/>
                
                
