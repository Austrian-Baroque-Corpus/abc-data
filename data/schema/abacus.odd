                        <attDef ident="ana" mode="change" usage="req">
                            <valList mode="replace" type="closed">
                                <valItem ident="#prose">
                                    <desc>declares that the element it is attached to is (mainly) written in prose</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="type" mode="change" usage="req">
                            <valList mode="replace" type="closed">
                                <valItem ident="book">
                                    <desc>declares that this text is conceptualised as a book (including empty pages etc.)</desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="lg" mode="change">
